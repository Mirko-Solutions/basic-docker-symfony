{
  "name": "mirko/symfony-api",
  "license": "MIT",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "src/Application"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/symfony": "6.3.*",
    "symfony/runtime": "6.3.*",
    "monolog/monolog": "^3.2",
    "symfony/orm-pack": "^2.4",
    "symfony/console": "^6.3",
    "symfony/form": "^6.3",
    "symfony/error-handler": "*"
  },
  "config": {
    "bin-dir": "bin",
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    }
  },
  "extra": {
    "symfony-app-dir": "src",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "public",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.3",
    "symfony/maker-bundle": "^1.47"
  }
}
